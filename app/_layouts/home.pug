					positionStyle: 'fixed',
					closeClass: 'LogIn-form_close-btn',
					onOpen: function() { setTimeout(function(){$('body').addClass('fixed'); }, 500); },
					onClose: function() { $('body').removeClass('fixed') }
									
				});
			});
