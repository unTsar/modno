				$('#mk-filter-popup-products').bPopup({
					positionStyle: 'fixed',
					closeClass: 'filter-popup-products_close-btn',
					onOpen: function() { 
						$('body').addClass('fixed');
					},
					onClose: function() { $('body').removeClass('fixed') }			
				});
