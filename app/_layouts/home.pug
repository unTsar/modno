			//- 		onOpen: function() { $('body').addClass('fixed'); },
			//- 		onClose: function() { $('body').removeClass('fixed') }			
			//- 	});
			//- });
			//- $('.mk-filter-drop-popup .unfixed-btn').click(function(){
			//- 	setTimeout(function(){$('body').addClass('fixed'); }, 500);
			//- });
			//- $('.mk-filter-drop-popup .img-close').click(function(){
			//- 	$('.mk-filter-popup .img-close').click();
			//- });

			$('.mk-filter-popup-line').click(function(){
				if($(this).find('.option-checkbox').is(':checked')){
					$(this).find('.option-checkbox').prop('checked', false);
					$(this).removeClass('checked');
				}else{
					$(this).find('.option-checkbox').prop('checked', true);
					$(this).addClass('checked');
				}
			});
			$('.mk-filter-popup-line .option-checkbox').click(function(){
				if($(this).is(':checked')){
					$(this).prop('checked', false);
					$(this).parents('.line').removeClass('checked');
				}else{
					$(this).prop('checked', true);
					$(this).parents('.line').addClass('checked');
				}
			});


			$('.footer-links-btn').click(function(){
				$('.footer-links-btn').not(this).removeClass('active');
				$('.footer-links-btn').not(this).next().slideUp(250);
				$(this).next().slideToggle(250);
				$(this).toggleClass('active');
			});

			




		});

