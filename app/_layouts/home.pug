						$('#profile-dropdown').removeClass('hide');
					}else{
						$('#profile-dropdown').fadeOut(100)
						$('#profile-dropdown').addClass('hide')
					}
				}else{
					if(existVerticalScroll() && $('body').css('top') === "0px"){ 
